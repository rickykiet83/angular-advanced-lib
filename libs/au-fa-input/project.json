{
  "name": "kiet-au-fa-input",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/au-fa-input/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/au-fa-input/ng-package.json",
        "tsConfig": "libs/au-fa-input/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/au-fa-input/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/libs/au-fa-input"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "pack": {
    "executor": "nx:run-commands",
    "dependsOn": ["build"],
    "options": {
      "command": "npm pack dist/libs/au-fa-input --pack-destination publish"
    }
  }
}
